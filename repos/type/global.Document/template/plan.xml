<?xml version="1.0" encoding="UTF-8"?>
<form>
	<go-to flag="success" action="[default]" />
	<go-to flag="fail" action="[same]" />
	<group label="Cabeçalho">
		<field type="File" column="image" label="Imagem" id="_IMAGE_">
			<mime-type>image/jpeg</mime-type>
			<mime-type>image/gif</mime-type>
			<mime-type>image/pjpeg</mime-type>
			<mime-type>image/png</mime-type>
		</field>
		<field type="Phrase" column="header" label="Texto" id="_HEADER_" />
	</group>
	<group label="Estagiário">
		<field type="Phrase" column="trainee_header" label="Cabeçalho" id="_TRAINEE_HEADER_" />
		<field type="PlainText" column="trainee_content" label="Conteúdo" id="_TRAINEE_CONTENT_" />
	</group>
	<group label="Supervisor">
		<field type="Phrase" column="supervisor_header" label="Cabeçalho" id="_SUPERVISOR_HEADER_" />
		<field type="PlainText" column="supervisor_content" label="Conteúdo" id="_SUPERVISOR_CONTENT_" />
	</group>
	<group label="Unidade">
		<field type="Phrase" column="unity_header" label="Cabeçalho" id="_UNITY_HEADER_" />
		<field type="PlainText" column="unity_content" label="Conteúdo" id="_UNITY_CONTENT_" />
	</group>
	<group label="Período">
		<field type="Phrase" column="period_header" label="Cabeçalho" id="_PERIOD_HEADER_" />
		<field type="PlainText" column="period_content" label="Conteúdo" id="_PERIOD_CONTENT_" />
	</group>
	<group label="Atividades">
		<field type="Phrase" column="activity_header" label="Cabeçalho" id="_ACTIVITY_HEADER_" />
		<field type="PlainText" column="orientations" label="Orientações" id="_ORIENTATIONS_" />
	</group>
	<group label="Assinaturas">
		<field type="PlainText" column="assign_1" label="Assinatura 1" id="_ASSIGN_1_" />
		<field type="PlainText" column="assign_2" label="Assinatura 2" id="_ASSIGN_2_" />
		<field type="PlainText" column="assign_3" label="Assinatura 3" id="_ASSIGN_3_" />
		<field type="PlainText" column="assign_4" label="Assinatura 4" id="_ASSIGN_4_" />
	</group>
	<group label="Rodapé">
		<field type="PlainText" column="footer_1" label="Esquerdo" id="_FOOTER_1_" />
		<field type="PlainText" column="footer_2" label="Direito" id="_FOOTER_2_" />
	</group>
</form>